{
  "parser": "typescript-eslint-parser",
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["compat"],
  "settings": {
    "polyfills": ["fetch", "intersectionobserver"]
  },
  "rules": {
    "compat/compat": "error"
  }
}
